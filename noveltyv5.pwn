CMD:ayarla(playerid, params[])
{
	if (PlayerData[playerid][pAdmin] < 3) return 0;

	static
	    userid,
	    type[16],
	    amount[32];

	if (sscanf(params, "us[16]S()[32]", userid, type, amount))
 	{
	 	SendSyntaxMessage(playerid, "/ayarla [id] [ÝSÝMLER]");
	 	SendClientMessage(playerid, COLOR_GRAY, "[ÝSÝMLER]:{FFFFFF} cinsiyet, bakiye, yas, level, banka, tenrengi, dilsifirla, koken");
		return 1;
	}
	if (!IsPlayerConnected(userid))
	    return SendErrorMessage(playerid, "Belirttiðiniz oyuncu oyunda deðil!");

	if (!strcmp(type, "cinsiyet", true))
	{
	    if (isnull(amount) || strval(amount) < 1 || strval(amount) > 2)
	        return SendSyntaxMessage(playerid, "/ayarla [id] [cinsiyet] [1: erkek - 2: kadýn]");

		PlayerData[userid][pGender] = strval(amount);

		if (PlayerData[userid][pGender] == 1)
			SendServerMessage(playerid, "%s adlý oyuncunun cinsiyetini 'Erkek' olarak deðiþtirdin.", ReturnName(userid, 0));

		else if (PlayerData[userid][pGender] == 2)
			SendServerMessage(playerid, "%s adlý oyuncunun cinsiyetini 'Kadýn' olarak deðiþtirdin.", ReturnName(userid, 0));
	}
	else if (!strcmp(type, "level", true))
	{
	    if(!IsPlayerAdmin(playerid)) return 1;
	    if (isnull(amount))
	        return SendSyntaxMessage(playerid, "/ayarla [id/isim] [level] [yeni level]");

		PlayerData[userid][pLevel] = strval(amount);
		SendServerMessage(playerid, "%s adlý oyuncunun levelini %d olarak deðiþtirdin.", ReturnName(userid, 0), strval(amount));
	}
	else if (!strcmp(type, "banka", true))
	{
	    if(!IsPlayerAdmin(playerid)) return 1;
	    if (isnull(amount))
	        return SendSyntaxMessage(playerid, "/ayarla [id/isim] [banka] [yeni banka parasý miktarý]");

		PlayerData[userid][pBankMoney] = strval(amount);
		SendServerMessage(playerid, "%s adlý oyuncunun banka parasýný %d olarak deðiþtirdin.", ReturnName(userid, 0), strval(amount));
	}
	else if (!strcmp(type, "bakiye", true))
	{
	if(!IsPlayerAdmin(playerid)) return 1;
	    if (isnull(amount))
	        return SendSyntaxMessage(playerid, "/ayarla [id] [bakiye] [Yeni Bakiye]");

		PlayerData[userid][pVCoin] = strval(amount);
		SendServerMessage(playerid, "%s adlý oyuncunun bakiyesini \"%s\" olarak deðiþtirdin.", ReturnName(userid, 0), amount);
	}
	else if (!strcmp(type, "yas", true))
	{
	    if (isnull(amount))
	        return SendSyntaxMessage(playerid, "/ayarla [id] [yas] [yeni yaþ]");

		PlayerData[userid][pYas] = strval(amount);
		SendServerMessage(playerid, "%s adlý oyuncunun bakiyesini \"%s\" olarak deðiþtirdin.", ReturnName(userid, 0), amount);
	}
	else if (!strcmp(type, "tenrengi", true))
	{
	    if (isnull(amount) || strlen(amount) > 32)
	        return SendSyntaxMessage(playerid, "/ayarla [id] [tenrengi] [Yeni Ten Rengi (Beyaz, siyah)]");

		format(PlayerData[userid][pTenRengi], 32, amount);
		SendServerMessage(playerid, "You have set %s's tenrengi to \"%s\".", ReturnName(userid, 0), amount);
	}
	else if (!strcmp(type, "dilsifirla", true))
	{
		PlayerData[userid][pEkstraDil] = 0;
		SendServerMessage(playerid, "%s adlý oyuncunun dilini sýfýrladýnýz.", ReturnName(userid, 0));
	}
	else if (!strcmp(type, "koken", true))
	{
