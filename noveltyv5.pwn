Dialog:AracCeza(playerid, response, listitem, inputtext[])
{
	if(response)
	{
		if(isnull(inputtext)) return SendErrorMessage(playerid, "Geçerli plaka giriniz.");
        new veri;
	    for(new x = 0; x < MAX_VEHICLES; x++)
	 	{
		    new aid = Car_GetID(x);
		    if(aid == -1) continue;
		    if(!strcmp(inputtext, CarData[aid][carPlaka], true))
		    {
		  		PlayerData[playerid][pAracCeza] = aid;
		    	Dialog_Show(playerid, AracCezaYaz, DIALOG_STYLE_INPUT, "Araç Ceza Yazma", "Lütfen ceza miktarýný giriniz:", "Ýlerle", "<< Geri");
				veri++;
			}
