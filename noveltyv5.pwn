CMD:akurtar(playerid, params[])
{
	if(tiryapiyor[playerid] >= 1 || PlayerData[playerid][pPizzaBasladimi] >= 1)
		return SendErrorMessage(playerid, "Þu anda bu komutu kullanamazsýn.");

	if (PlayerData[playerid][pYukAldi]) return SendErrorMessage(playerid, "Þu anda bu komutu kullanamazsýn.");
	if (PlayerData[playerid][pCPHapis] == 1) return SendErrorMessage(playerid, "Þu anda bu komutu kullanamazsýnýz.");

    new aid;

	if(sscanf(params, "d", aid))
		return SendSyntaxMessage(playerid, "/akurtar [Araç ID]");

	if(aid < 1 || aid > MAX_VEHICLES) return 1;

    for(new x = 0; x < MAX_VEHICLES; x++)
	{
	    if(x == aid)
	    {
			aid = Car_GetID(x);
			if(Car_IsOwner(playerid, aid) || Car_Anahtar(playerid, aid))
			{
				if (CarData[aid][carParcalatTime] <= 0) return SendErrorMessage(playerid, "Bu araba parçalatýlmamýþ!");
				new model = CarData[aid][carModel], fiyat = AracFiyati(model) / 6;
				if (GetMoney(playerid) < fiyat) return SendErrorMessage(playerid, "Yeteri kadar paranýz yok!");
				GiveMoney(playerid, -fiyat / 6);
				CarData[aid][carParcalatTime] = 0;
				RespawnVehicle(aid);
				SendServerMessage(playerid, "%s ödediniz ve aracýnýz respawnlandý!", FormatNumber(fiyat));
				break;
	   		}
	  		else
			{ SendErrorMessage(playerid, "Bu aracýn sahibi deðilsiniz."); }
	  	}
	}
	return 1;
}




