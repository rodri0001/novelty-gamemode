	if (GetVehicleModel(GetPlayerVehicleID(playerid)) != 423 && GetVehicleModel(GetPlayerVehicleID(playerid)) != 588)
 		return SendErrorMessage(playerid, "Gýda aracýnda deðilsin.");

	static
	    userid,
