forward KickHouse(playerid, id);
public KickHouse(playerid, id)
{
if (GetFactionType(playerid) != FACTION_POLICE && GetFactionType(playerid) != FACTION_INTEL || House_Nearest(playerid) != id) return 0;
HouseData[id][houseLocked] = 0;
House_Save(id);
HouseData[id][housePass][0] = '\0';
House_Refresh(id);
SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s kapýyý kýrdý.", ReturnName(playerid, 0));
ClearAnimations(playerid);
return 1;
}
