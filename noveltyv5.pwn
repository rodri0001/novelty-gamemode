	    return SendErrorMessage(playerid, "Baygýnken veya ölüyken bu komutu kullanamazsýn.");

	if (GetFactionType(playerid) == FACTION_GOV || GetFactionType(playerid) == FACTION_POLICE || GetFactionType(playerid) == FACTION_INTEL)
