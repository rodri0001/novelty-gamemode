public HealthCheck(playerid)
{
	new Float:hh2;
	GetPlayerHealth(playerid,hh2);
	beingchecked[playerid] = 0;
	if(hh2 == 100)
	{
		//Player Is Hacking
		IsHacking[playerid] +=1;
		SetTimerEx("HackTest",3000,false,"i",playerid); // Player is hacking, checks again in 3 seconds to clarify
		// Add a message here sent to admins to let them know the player was tested positive for health hacks.
		if(IsHacking[playerid] == 3) //Amount of successfull checks before punishing player
		{
		    //SendServerMessage(playerid, "Hile tespit edildi, ATILDIN!");
		    //KickEx(playerid);
		    // These lines are the punishment for when a player is caught health hacking 3 times ^^^
		}
	}
	else
	{
	    //Player Isn't Hacking
	    SetPlayerHealth(playerid,hh1[playerid]); //Restore player's original health
	}
	return 1;
}
