	if (Car_IsOwner(playerid, id) || PlayerData[playerid][pAdmin] >= 999 || Car_Birlik(playerid, id) || Car_Anahtar(playerid, id) || Car_Vip(playerid, id) || Car_Meslek(playerid, id) ||  vehicleid == PlayerData[playerid][pTestCar] || (PlayerData[playerid][pFaction] != -1 && GetFactionType(playerid) != FACTION_GANG && CarData[id][carFaction] == GetFactionType(playerid)))
