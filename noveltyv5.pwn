{
if(ch == 'q') ch = 'Q';
else if(ch == 'w') ch = 'W';
else if(ch == 'e') ch = 'E';
else if(ch == 'r') ch = 'R';
else if(ch == 't') ch = 'T';
else if(ch == 'y') ch = 'Y';
else if(ch == 'u') ch = 'U';
else if(ch == 'ý') ch = 'I';
else if(ch == 'o') ch = 'O';
else if(ch == 'p') ch = 'P';
else if(ch == 'ð') ch = 'Ð';
else if(ch == 'ü') ch = 'Ü';
else if(ch == 'a') ch = 'A';
else if(ch == 's') ch = 'S';
else if(ch == 'd') ch = 'D';
else if(ch == 'f') ch = 'F';
