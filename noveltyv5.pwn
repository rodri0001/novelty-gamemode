	    return SendSyntaxMessage(playerid, "/sms [telefon numarasý] [mesaj]");

	if (!number)
	    return SendErrorMessage(playerid, "Belirtilen numara bir hizmete ait deðil.");

	if ((targetid = GetNumberOwner(number)) != INVALID_PLAYER_ID)
