	    else
		{
		    new id = Location_GetID(playerid, inputtext);

		    if (id != -1) {
		        PlayerData[playerid][pSelectedSlot] = id;

		        Dialog_Show(playerid, LocationInfo, DIALOG_STYLE_LIST, inputtext, "Ýþaretle\nLokasyonu Sil", "Seç", "<< Geri");
			}
		}
	}
	else cmd_gps(playerid, "\1");
