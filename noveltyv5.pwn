                AgriKesici[vehicleid] = false;
			    SendNearbyMessage(playerid, 30.0, COLOR_CYAN, "** %s araçtan aðrý kesici alýr ve yutar.", ReturnName(playerid, 0));
			}
			return 1;
		}
	    if(CruiserData[vehicleid][slotid][KalanMermi] <= 0)
		{
