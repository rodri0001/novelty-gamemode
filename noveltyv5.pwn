CMD:rozet(playerid, params[])
{
if (GetFactionType(playerid) == FACTION_POLICE) return SendErrorMessage(playerid, "LSPD sadece /pdduty kullanabilir, veya pd dolabýndan iþbaþý yapabilir.");

	if (GetFactionType(playerid) == FACTION_GANG)
		return SendErrorMessage(playerid, "Bu komutu sadece devlet kurumlarý kullanabilir !");

	if(PlayerData[playerid][pOnDuty])
	{
	    PlayerData[playerid][pOnDuty] = false;
	    SetPlayerColor(playerid, 0xFFFFFFFF);
		SendServerMessage(playerid, "Rozet gizlendi.");
	}
	else {
	    PlayerData[playerid][pOnDuty] = true;
	    SendServerMessage(playerid, "Rozet gösterildi.");
	    if (GetFactionType(playerid) == FACTION_MEDIC)
	    {
	    SetPlayerColor(playerid, 0xFF8080FF);
	    }
	}
	if (GetFactionType(playerid) == FACTION_POLICE) {
	SetPlayerColor(playerid, COLOR_RADIO);
	PlayerData[playerid][pOnDuty] = true;
	}

    return 1;
}
