		if(veri == 0) return SendErrorMessage(playerid, "Hiç veri bulunamadý.");
	}
	else Dialog_Show(playerid, GenelIslemler, DIALOG_STYLE_LIST, "{0033CC}LSPD - Mobile Data Computer", "Plakaya Ceza Ýþlemi\nPlaka Takip\nLokasyon Bul\nKiþi Sorgula", "Seç", "{FF6347}Kapat");
	return 1;
}
