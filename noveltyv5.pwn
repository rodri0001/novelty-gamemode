CMD:adminyap(playerid, params[])
{
	static
		userid,
	    level;
if (!IsPlayerAdmin(playerid))
	    return 0;

	if (sscanf(params, "ud", userid, level))
		return SendSyntaxMessage(playerid, "/adminyap [id] [Seviye]");

	if (!IsPlayerConnected(userid))
	    return SendErrorMessage(playerid, "Belirttiðiniz oyuncu oyunda deðil !");

if (level < 0 || level > 4)
	    return SendErrorMessage(playerid, "Maksimum 4 seviye adminlik verilebilir.");

	if (level > PlayerData[userid][pAdmin])
	{
	    SendAdminAction(playerid, "%s adlý þahýsa adminlik seviyesi verdin. (%d)", ReturnName(userid, 0), level);
	    SendAdminAction(userid, "%s adlý yetkili sana yetki verdi ! (%d)", ReturnName(playerid, 0), level);
	}
	else
	{
	    SendAdminAction(playerid, "%s adlý oyuncuyu (%d) seviye yetkili yaptýnýz.", ReturnName(userid, 0), level);
	    SendAdminAction(userid, "%s adlý yetkili seni (%d) seviye yetkili yaptý.", ReturnName(playerid, 0), level);
	}
	PlayerData[userid][pAdmin] = level;
 	Log_Write("logs/admin_log.txt", "[%s] %s has set %s's admin level to %d.", ReturnDate(), ReturnName(playerid, 0), ReturnName(userid, 0), level);

	return 1;
}

