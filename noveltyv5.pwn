else if(ch == 'þ') ch = 'Þ';
else if(ch == 'i') ch = 'Ý';
else if(ch == 'z') ch = 'Z';
else if(ch == 'x') ch = 'X';
else if(ch == 'c') ch = 'C';
else if(ch == 'v') ch = 'V';
