CMD:me(playerid, params[])
{
    if(GetPVarInt(playerid, "TV") == 1) return 1;
	if (isnull(params))
	    return 0;

	new action[256], string[256];
    strdel(action, 0, 256);
	strcat(action, params);
    if(strfind(action, "\"", true, 1) != -1)
	{
			new ilkdenden = strfind(action, "\"", true, 1);
			new sonrakidenden = strfind(action, "\"", true, ilkdenden+1);
			strins(action, "{FFFFFF}", ilkdenden);
			strins(action, "{C2A2DA}", sonrakidenden+9);
	}
	if (strlen(action) > 120) {
 	    SendNearbyMessage(playerid, 15.0, COLOR_CYAN, "* %s %.120s", ReturnName(playerid, 0), action); // ciyan
	    SendNearbyMessage(playerid, 15.0, COLOR_CYAN, "...%s", action[120]);
	}
	else {
	    SendNearbyMessage(playerid, 15.0, COLOR_CYAN, "* %s %s", ReturnName(playerid, 0), action);
	}
	format(string, sizeof(string), "* %s %s", ReturnName(playerid, 0), action);
 	SetPlayerChatBubble(playerid, string, COLOR_CYAN, 15.0, 6000);
	return 1;
}
