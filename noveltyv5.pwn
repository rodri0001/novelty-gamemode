CMD:garajduzenle(playerid, params[])
{
	static
	    id,
	    type[24],
	    string[128];

	if (PlayerData[playerid][pAdmin] < 3)
	    return 0;

	if (sscanf(params, "ds[24]S()[128]", id, type, string))
 	{
	 	SendSyntaxMessage(playerid, "/garajduzenle [id] [eylem]");
	    SendClientMessage(playerid, COLOR_YELLOW, "[EYLEMLER]:{FFFFFF} lokasyon, interior, virtual, kilit, ad");
		return 1;
	}
	if ((id < 0 || id >= MAX_GARAGES) || !GarageData[id][garageExists])
	    return SendErrorMessage(playerid, "Geçersiz Garaj ID!");

	if (!strcmp(type, "lokasyon", true))
	{
	    GetPlayerPos(playerid, GarageData[id][garagePos][0], GarageData[id][garagePos][1], GarageData[id][garagePos][2]);
		Garage_Refresh(id);
		Garage_Save(id);
		SendServerMessage(playerid, "Garajýn lokasyonu düzenlendi ve veritabanýna kaydedildi!");
	}
	else if (!strcmp(type, "interior", true))
	{
	    GetPlayerPos(playerid, GarageData[id][garageInt][0], GarageData[id][garageInt][1], GarageData[id][garageInt][2]);
        foreach (new i : Player)
		{
			if (PlayerData[i][pGarage] == GarageData[id][garageID])
			{
			PlayerData[playerid][pLegalTP] = 1;
				SetPlayerPos(i, GarageData[id][garageInt][0], GarageData[id][garageInt][1], GarageData[id][garageInt][2]);
				SetCameraBehindPlayer(i);
			}
		}
		Garage_Save(id);
		SendServerMessage(playerid, "Garajýn interioru düzenlendi ve veritabanýna kaydedildi!");
	}
	else if (!strcmp(type, "kilit", true))
	{
	    new locked;

	    if (sscanf(string, "d", locked))
	        return SendSyntaxMessage(playerid, "/garajduzenle [id] [kilit] [0/1]");

		if (locked < 0 || locked > 1)
		    return SendErrorMessage(playerid, "Geçersiz rakam!");

	    GarageData[id][garageLocked] = locked;
	    Garage_Refresh(id);
	    Garage_Save(id);

	    if (locked) {
			SendServerMessage(playerid, "Garaj kilitlendi!");
		} else {
		    SendServerMessage(playerid, "Garajýn kilidi açýldý!");
		}
	}
	else if (!strcmp(type, "virtual", true))
	{
	    new worldid;

	    if (sscanf(string, "d", worldid))
	        return SendSyntaxMessage(playerid, "/garajduzenle [id] [virtual] [yeni VW]");

	    GarageData[id][garageVirtual] = worldid;

		foreach (new i : Player) if (Garage_Inside(i) == id) {
			SetPlayerVirtualWorld(i, worldid);
		}
		Garage_Save(id);
		SendServerMessage(playerid, "Garajýn virtual world parametresi güncellendi.");
	}
	else if (!strcmp(type, "ad", true))
	{
	    new name[32];

	    if (sscanf(string, "s[32]", name))
	        return SendSyntaxMessage(playerid, "/garajduzenle [id] [ad] [yeni ad]");

	    format(GarageData[id][garageName], 32, name);

	    Garage_Refresh(id);
	    Garage_Save(id);

		SendServerMessage(playerid, "Garajýn ismi %s olarak güncellendi!", name);
    }
	return 1;
}
CMD:editentrance(playerid, params[])
